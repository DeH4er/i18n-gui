    "langmap": "^0.0.16",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-spring": "^9.4.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-vtree": "^3.0.0-beta.3",
    "react-window": "^1.8.6",
    "styletron-engine-atomic": "^1.4.8",
    "styletron-react": "^6.0.2",
    "uuid": "^8.3.2",
    "xss": "^1.0.10"
